						<key>Text</key>
						<string>tell application "Finder"
	set finderWindowNames to the name of the Finder windows
end tell

tell application "Keyboard Maestro Engine"
	-- initialize
	set finderWindow1Ref to make variables with properties {name:"finderWindow1"}
	set finderWindow2Ref to make variables with properties {name:"finderWindow2"}
	set finderWindow3Ref to make variables with properties {name:"finderWindow3"}
	set finderWindow4Ref to make variables with properties {name:"finderWindow4"}
	
	-- set variables
	if length of finderWindowNames is 1 then
		set value of finderWindow1Ref to item 1 of finderWindowNames
	else if length of finderWindowNames is 2 then
		set value of finderWindow1Ref to item 1 of finderWindowNames
		set value of finderWindow2Ref to item 2 of finderWindowNames
	else if length of finderWindowNames is 3 then
		set value of finderWindow1Ref to item 1 of finderWindowNames
		set value of finderWindow2Ref to item 2 of finderWindowNames
		set value of finderWindow3Ref to item 3 of finderWindowNames
	else if length of finderWindowNames â‰¥ 4 then
		set value of finderWindow1Ref to item 1 of finderWindowNames
		set value of finderWindow2Ref to item 2 of finderWindowNames
		set value of finderWindow3Ref to item 3 of finderWindowNames
		set value of finderWindow4Ref to item 4 of finderWindowNames
	end if
end tell</string>
